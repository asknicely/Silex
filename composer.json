{
    "name": "silex/silex",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": ["microframework"],
    "homepage": "https://github.com/asknicely/silex",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        },
        {
            "name": "Aaron Cooper",
            "email": "aaron.cooper@asknice.ly"
        }
    ],
    "require": {
        "php": ">=7.4|^8.0",
        "pimple/pimple": "^3.2",
        "symfony/event-dispatcher": "^4.4",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^4.4",
        "symfony/routing": "^4.4"
    },
    "require-dev": {
        "doctrine/dbal": "^2.13",
        "fig/link-util": "^1.1",
        "monolog/monolog": "^1.4.1",
        "phpunit/phpunit": "^9.6",
        "rector/rector-prefixed": "^0.9.31",
        "swiftmailer/swiftmailer": "^6.3.0",
        "symfony/asset": "^4.4",
        "symfony/browser-kit": "^4.4",
        "symfony/config": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug": "^4.4",
        "symfony/doctrine-bridge": "^4.4",
        "symfony/dom-crawler": "^4.4",
        "symfony/expression-language": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/form": "^4.4",
        "symfony/intl": "^4.4",
        "symfony/monolog-bridge": "^4.4",
        "symfony/options-resolver": "^4.4",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/process": "^4.4",
        "symfony/security": "4.4.44",
        "symfony/serializer": "^4.4",
        "symfony/translation": "^4.4",
        "symfony/twig-bridge": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/var-dumper": "^4.4",
        "symfony/web-link": "^4.4",
        "twig/twig": "^3"
    },
    "replace": {
        "silex/api": "self.version",
        "silex/providers": "self.version"
    },
    "autoload": {
        "psr-4": { "Silex\\": "src/Silex" }
    },
    "autoload-dev" : {
        "psr-4": { "Silex\\Tests\\" : "tests/Silex/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
